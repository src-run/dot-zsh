        "setups":
        {
            "_enabled": true,
            "internal_variables": {
                "NVM_DIR": "${NVM_DIR:-/opt/nvm}"
            }
        },
        "editor":
        {
            "internal_session": "subl",
            "external_session": "nano"
        },
        "key_bindings":
        {
            "history_substring_search_up": "kcuu1",
            "history_substring_search_down": "kcud1"
        },
        "aliases":
        {
            "_enabled": true,
            "new_simple_commands": true,
            "set_simple_commands":
            {
                "ccat": "pygmentize -g",
                "vcat": "pygmentize -g -O style=colorful,linenos=1",
                "grep": "grep --color=auto",
                "fgrep": "fgrep --color=auto",
                "egrep": "egrep --color=auto",
                "ls": "ls --color=auto -I.gvfs",
                "la": "ls --color=auto -I.gvfs -AlFhB",
                "ll": "ls --color=auto -I.gvfs -Al",
                "lf": "ls --color=auto -I.gvfs -AF",
                "lc": "ls --color=auto -I.gvfs -FC"
            },
            "new_ssh_connections": true,
            "set_ssh_connections":
